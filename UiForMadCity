task.wait(2) --Wait for Mad City Logo to load
Base.BackgroundTransparency = 0
Main.BackgroundTransparency = 0
DropShadow.ImageTransparency = 0.500
UIStroke_1.Thickness = 2
task.spawn(function()
	game:GetService("TweenService"):Create(ImageLabel, TweenInfo.new(0.5), { ImageTransparency = 0 }):Play()
	UIStroke_2.Transparency = 0
	Cash.BackgroundTransparency = 0
	Amount.TextTransparency = 0
	game:GetService("TweenService"):Create(Cash, TweenInfo.new(0.25), { Size = UDim2.new(0.9, 0, 0.125, 0) }):Play()
	task.wait(0.1)
	UIStroke_3.Transparency = 0
	XP.BackgroundTransparency = 0
	Amount_2.TextTransparency = 0
	game:GetService("TweenService"):Create(XP, TweenInfo.new(0.25), { Size = UDim2.new(0.9, 0, 0.125, 0) }):Play()
	task.wait(0.1)
	UIStroke_4.Transparency = 0
	Rank.BackgroundTransparency = 0
	Amount_3.TextTransparency = 0
	game:GetService("TweenService"):Create(Rank, TweenInfo.new(0.25), { Size = UDim2.new(0.9, 0, 0.125, 0) }):Play()
	task.wait(0.1)
	UIStroke_5.Transparency = 0
	Earned.BackgroundTransparency = 0
	EarnedText.TextTransparency = 0
	Cash_2.TextTransparency = 0
	Rank_2.TextTransparency = 0
	game:GetService("TweenService"):Create(Earned, TweenInfo.new(0.25), { Size = UDim2.new(0.9, 0, 0.125, 0) }):Play()
	task.wait(0.1)
	UIStroke_6.Transparency = 0
	Credits.BackgroundTransparency = 0
	HubName.TextTransparency = 0
	Code.TextTransparency = 0
	game:GetService("TweenService"):Create(Credits, TweenInfo.new(0.25), { Size = UDim2.new(0.9, 0, 0.125, 0) }):Play()
	task.wait(1)
	game:GetService("TweenService"):Create(Cash, TweenInfo.new(0.25), { BackgroundTransparency = 1 }):Play()
	task.wait(0.1)
	game:GetService("TweenService"):Create(XP, TweenInfo.new(0.25), { BackgroundTransparency = 1 }):Play()
	task.wait(0.1)
	game:GetService("TweenService"):Create(Rank, TweenInfo.new(0.25), { BackgroundTransparency = 1 }):Play()
	task.wait(0.1)
	game:GetService("TweenService"):Create(Earned, TweenInfo.new(0.25), { BackgroundTransparency = 1 }):Play()
	task.wait(0.1)
	game:GetService("TweenService"):Create(Credits, TweenInfo.new(0.25), { BackgroundColor3 = Color3.new(0, 0.784314, 0) }):Play()
	game:GetService("TweenService"):Create(UIStroke_6, TweenInfo.new(0.25), { Color = Color3.new(0, 0.784314, 0) }):Play()
end)
task.spawn(function()
	local OldCash = game.Players.LocalPlayer.Cash.Value
	local OldRank = game.Players.LocalPlayer.Level.Value
	while task.wait(0.001) do
		Amount.Text = "Cash: $" .. tostring(game.Players.LocalPlayer.Cash.Value)
		Amount_2.Text = "XP: "..string.sub(game.Players.LocalPlayer.PlayerGui.MainUI.LeftMain.XP.Container.XpAmount.Text, 1, tonumber(string.find(game.Players.LocalPlayer.PlayerGui.MainUI.LeftMain.XP.Container.XpAmount.Text, "X") - 2))
		Amount_3.Text = "Rank:" .. tostring(game.Players.LocalPlayer.Level.Value)
		Cash_2.Text = "Cash: " .. game.Players.LocalPlayer.Cash.Value - OldCash
		Rank_2.Text = "Rank: " .. game.Players.LocalPlayer.Level.Value - OldRank
	end
end)
task.spawn(function()
	local TweenService = game:GetService("TweenService")
	local Duration = 5
	local RgbColors = {
	    Color3.new(1, 0, 0),
	    Color3.new(1, 0.5, 0),
	    Color3.new(1, 1, 0),
	    Color3.new(0, 1, 0),
	    Color3.new(0, 0, 1),
	    Color3.new(0.5, 0, 1),
	    Color3.new(1, 0, 0)
	}
	local function RgbChange()
		for i, v in RgbColors do
			local TweenInfo = TweenInfo.new(Duration/(#RgbColors - 1), Enum.EasingStyle.Linear)
			local Tween_1 = TweenService:Create(DropShadow, TweenInfo, {ImageColor3 = v})
			local Tween_2 = TweenService:Create(UIStroke_1, TweenInfo, {Color = v})
			local Tween_3 = TweenService:Create(Credits, TweenInfo, {BackgroundColor3 = v})
			local Tween_4 = TweenService:Create(UIStroke_6, TweenInfo, {Color = v})
			Tween_1:Play()
			Tween_2:Play()
			Tween_3:Play()
			Tween_4:Play()
			task.wait(Duration/(#RgbColors))
		end
	end
	while true do
		RgbChange()
	end
end)
